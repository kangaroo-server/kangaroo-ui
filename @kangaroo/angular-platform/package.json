{
  "$schema": "../../node_modules/ng-packagr/package.schema.json",
  "name": "@kangaroo/angular-platform",
  "version": "1.0.0",
  "description": "API platform contracts required by the Kanagaroo ecosystem.",
  "license": "Apache-2.0",
  "author": {
    "name": "Michael Krotscheck",
    "email": "krotscheck@gmail.com",
    "url": "https://krotscheck.net/"
  },
  "typings": "lib/kangaroo-angular-platform.d.ts",
  "main": "lib/bundles/kangaroo-angular-platform.umd.js",
  "module": "lib/esm5/kangaroo-angular-platform.js",
  "es2015": "lib/esm2015/kangaroo-angular-platform.js",
  "metadata": "lib/kangaroo-angular-platform.metadata.json",
  "scripts": {
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "build": "ng-packagr -p package.json",
    "nsp": "nsp check ./"
  },
  "ngPackage": {
    "dest": "lib",
    "lib": {
      "entryFile": "src/index.ts",
      "languageLevel": [
        "dom",
        "es2017"
      ],
      "comments": "none",
      "licensePath": "LICENSE"
    }
  },
  "devDependencies": {
    "@angular/cli": "~1.7.0",
    "@angular/compiler": "~5.2.4",
    "@angular/compiler-cli": "~5.2.4",
    "@types/jasmine": "~2.8.6",
    "@types/node": "~8.9.0",
    "codelyzer": "~4.1.0",
    "jasmine-core": "~2.99.1",
    "karma": "~2.0.0",
    "karma-browserstack-launcher": "~1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.1",
    "karma-jasmine": "~1.1.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-spec-reporter": "~0.0.32",
    "karma-webdriver-launcher": "~1.0.5",
    "ng-packagr": "~2.0.0",
    "nsp": "~3.2.1",
    "tslint": "~5.9.1",
    "tslint-microsoft-contrib": "~5.0.2",
    "typescript": "~2.7.2"
  },
  "dependencies": {
    "@angular/common": "~5.2.4",
    "@angular/core": "~5.2.4",
    "rxjs": "~5.5.6",
    "zone.js": "~0.8.20"
  }
}
