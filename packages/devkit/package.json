{
  "name": "@kangaroo/devkit",
  "version": "1.0.0",
  "description": "Consistent development tools for the kangaroo ecosystem.",
  "license": "Apache-2.0",
  "private": true,
  "author": {
    "name": "Michael Krotscheck",
    "email": "krotscheck@gmail.com",
    "url": "https://krotscheck.net/"
  },
  "scripts": {
    "lint.tslint": "tslint -p ./src/tslint/tsconfig.json",
    "lint.tslint.ci": "tslint -p ./src/tslint/tsconfig.json --format checkstyle --force > ./reports/tslint/checkstyle-result.xml",
    "lint.karma": "tslint -p ./src/karma/tsconfig.json",
    "lint.karma.ci": "tslint -p ./src/karma/tsconfig.json --format checkstyle --force > ./reports/karma/checkstyle-result.xml",
    "lint.protractor": "tslint -p ./src/protractor/tsconfig.json",
    "lint.protractor.ci": "tslint -p ./src/protractor/tsconfig.json --format checkstyle --force > ./reports/protractor/checkstyle-result.xml",
    "lint": "yarn lint.tslint && yarn lint.karma && yarn lint.protractor",
    "lint.ci": "yarn lint.tslint.ci && yarn lint.karma.ci && yarn lint.protractor.ci",
    "test": "jasmine",
    "test.ci": "jasmine",
    "build.tslint": "tsc -p ./src/tslint/tsconfig.json",
    "build.karma": "tsc -p ./src/karma/tsconfig.json",
    "build.protractor": "tsc -p ./src/protractor/tsconfig.json",
    "build": "yarn build.tslint && yarn build.karma && yarn build.protractor",
    "nsp": "nsp check ./"
  },
  "dependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler": "~5.2.9",
    "@angular/compiler-cli": "~5.2.9",
    "@angular/core": "~5.2.9",
    "@types/jasmine": "~2.8.6",
    "@types/karma": "~1.7.3",
    "@types/node": "~8.9.0",
    "codelyzer": "~4.2.1",
    "jasmine-core": "^2.99",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-browserstack-launcher": "~1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-junit-reporter": "~1.2.0",
    "karma-spec-reporter": "~0.0.32",
    "karma-typescript": "~3.0.12",
    "karma-webdriver-launcher": "~1.0.5",
    "ng-packagr": "~2.4.2",
    "nsp": "~3.2.1",
    "protractor": "~5.3.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-microsoft-contrib": "~5.0.2",
    "typescript": "~2.8.1"
  },
  "devDependencies": {
    "jasmine": "~3.1.0"
  }
}
